<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="700.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.julionxn.jnlauncher.controllers.LoginViewController">
   <children>
      <AnchorPane fx:id="dragable" layoutX="25.0" layoutY="25.0" prefHeight="350.0" prefWidth="350.0" />
      <Button fx:id="loginBtn" layoutX="420.0" layoutY="180.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="200.0" style="-fx-font-size: 18px" styleClass="sidebar-btn" text="Login" />
      <Button fx:id="logoutBtn" layoutX="580.0" layoutY="210.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#logout" prefHeight="40.0" prefWidth="40.0" styleClass="sidebar-btn" text="X" />
      <Button fx:id="enterBtn" layoutX="420.0" layoutY="210.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#enter" prefHeight="40.0" prefWidth="150.0" style="-fx-font-size: 18px" styleClass="sidebar-btn" text="Go!" textAlignment="CENTER" textOverrun="CLIP" />
      <Button fx:id="closeBtn" layoutX="310.0" layoutY="50.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#close" prefHeight="40.0" prefWidth="40.0" styleClass="login-close-btn" text="X" />
      <ImageView fx:id="headView" fitHeight="40.0" fitWidth="40.0" layoutX="500.0" layoutY="160.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
